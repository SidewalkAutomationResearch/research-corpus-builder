  font-size: var(--cl-font-size-small);
  line-height: var(--cl-line-height-small);
}

.msf-navigation {
  position: relative;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 2rem;
}

